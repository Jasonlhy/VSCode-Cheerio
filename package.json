{
    "name": "vscode-cheerio",
    "displayName": "vscode-cheerio",
    "description": "Integrate cheerio with vscode to manipulate HTML",
    "version": "0.0.1",
    "publisher": "jasonlhy",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.cheerioEvalCommand"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.cheerioEvalCommand",
                "title": "Cheerio: Eval Command"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.2"
    }
}
